{
	"name": "@viaz/types",
	"version": "0.2.4",
	"description": "viaz types",
	"type": "module",
	"main": "./index.ts",
	"module": "./index.ts",
	"types": "./index.ts",
	"exports": {
		".": {
			"default": "./index.ts",
			"require": "./index.ts",
			"module": "./index.ts"
		}
	},
	"files": [
