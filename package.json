    "@vueuse/core": "^10.9.0",
    "ant-design-vue": "^4.1.2",
    "axios": "^1.6.7",
    "codemirror": "^6.0.1",
    "cz-git": "^1.9.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
