{
  "name": "viazs",
  "private": "true",
  "version": "0.0.2-alpha.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "pnpm --filter ./docs run dev",
    "play": "pnpm --filter ./play run dev",
    "build": "pnpm --filter ./packages/** run build",
    "build:viaz": "pnpm --filter ./packages/viaz run build",
    "build:dts": "tsx ./scripts/copy-dts.ts",
    "build:internals-build": "pnpm --filter ./internals/** run build",
    "clean": "run-p -l clean:dist clean:dependencies",
    "clean:dist": "rimraf ./packages/**/*/dist",
    "clean:test": "tsx ./scripts/clean.ts",
    "clean:docs": "rimraf ./docs/node_modules ./docs/pnpm-lock.yaml",
    "clean:dependencies": "tsx ./scripts/clean-dependencies.ts",
    "clean:all": "tsx ./scripts/clean.ts",
    "dependencies": "pnpm install -Dw",
    "install:dependencies": "pnpm --filter ./packages/** install --force",
    "install:docs": "pnpm --filter ./docs install",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "peerDependencies": {
    "vue": "^3.4.21"
  },
  "devDependencies": {
